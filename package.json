{
  "name": "vscode-img-fast",
  "displayName": "Img Fast",
  "description": "Quickly uploads clipboard images in markdown.",
  "version": "0.0.1",
  "publisher": "lvboda",
  "author": {
    "name": "Boda Lü",
    "email": "lv_boda@163.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lvboda/vscode-img-fast.git"
  },
  "homepage": "https://github.com/lvboda/vscode-img-fast/blob/master/README.md",
  "bugs": {
    "url": "https://github.com/lvboda/vscode-img-fast/issues"
  },
  "keywords": [
    "markdown",
    "image",
    "paste",
    "upload",
    "auto upload",
    "pic"
  ],
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:markdown",
    "onCommand:img-fast.upload",
    "onCommand:img-fast.delete"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "img-fast.upload",
        "title": "上传"
      },
      {
        "command": "img-fast.delete",
        "title": "删除"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Img Fast Configuration",
      "properties": {
        "img-fast.openPasteAutoUpload": {
          "type": "boolean",
          "default": true,
          "description": "Whether to enable automatic uploading of pasted images."
        },
        "img-fast.authorization": {
          "type": "string",
          "default": "BD1010110",
          "description": "Request authorization token."
        },
        "img-fast.imgRename": {
          "type": "string",
          "default": "${hash}-${yyyy}-${MM}-${dd}-${hh}-${mm}-${ss}-${timestamp}-${name}",
          "description": "Request form data key."
        },
        "img-fast.outputRename": {
          "type": "string",
          "default": "![${name}](${url})",
          "description": "Request form data key."
        },
        "img-fast.uploadUrl": {
          "type": "string",
          "default": "http://localhost:8000/",
          "description": "Request Upload api."
        },
        "img-fast.uploadMethod": {
          "type": "string",
          "enum": [
            "GET",
            "POST",
            "PUT",
            "DELETE"
          ],
          "default": "POST",
          "description": "Upload request method."
        },
        "img-fast.uploadFormDataKey": {
          "type": "string",
          "default": "img",
          "description": "Request form data key."
        },
        "img-fast.deletedFlag": {
          "type": "string",
          "enum": [
            "none",
            "url",
            "layout"
          ],
          "default": "layout",
          "description": "deleted flag."
        },
        "img-fast.deleteUrl": {
          "type": "string",
          "default": "http://localhost:8000/",
          "description": "Request delete api."
        },
        "img-fast.deleteMethod": {
          "type": "string",
          "enum": [
            "GET",
            "POST",
            "PUT",
            "DELETE"
          ],
          "default": "DELETE",
          "description": "Delete request method."
        },
        "img-fast.deleteQueryKey": {
          "type": "string",
          "default": "",
          "description": "Delete request query key."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.26.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "crypto": "^1.0.1",
    "electron-clipboard-ex": "^1.3.3",
    "form-data": "^4.0.0"
  }
}
